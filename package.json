{
  "name": "osrs-wiki-bucket-builder",
  "version": "1.0.0",
  "description": "A Package to build Query strings for the OSRS Wiki Bucket API",
  "keywords": [
    "osrs",
    "typescript"
  ],
  "homepage": "https://github.com/Dava96/osrs-wiki-bucket-builder",
  "bugs": {
    "url": "https://github.com/Dava96/osrs-wiki-bucket-builder/issues"
  },
  "author": "Dava96 <Dava96@github.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dava96/osrs-wiki-bucket-builder.git"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "dist/index.js",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.13"
  },
  "scripts": {
    "dev": "vitest",
    "build": "tsc",
    "ci": "npm run build && npm run check-format && npm run test",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "test": "vitest run",
    "local-release": "changeset version && changeset publish",
    "prepublishOnly": "npm run ci"
  }
}
